 def is_operator(token): 
  return token in ['+','-','*','/','^','=','(',')']  
def is_operand(token): 
  return token.replace('.','',1).isdigit() or token.isalpha() 
def tokenize(text): 
  """ tokenize the text into a list of clean tokens. """   tokens=[]   current = ''  
for ch in text: 
    if ch in '+-*/^=()':       
      if current: 
        tokens.append(current.strip()) 
        current = ''tokens.append(ch)    
elif ch.isalnum() or ch == '.': 
  current += ch else:       
  if current: 
    tokens.append(current.strip()) 
        current = ''tokens.append(' ')   
if current:    
 tokens.append(current.strip())   
  return tokens 
def extract_math_expressions(tokens): 
  expression = []   current_expr = []   expression_started = False 
  valid_tokens = set(['=','+','-','*','/','^','(',')']) 
  for token in tokens: 
    token = token.strip()     
  if not token:       
   continue 
 if is_operand(token) or is_operator(token):       
  if not expression_started:         
    expression_started = True       
    current_expr.append(token)     
else:       
if expression_started and len(current_expr) > 3: 
expression.append(''.join(current_expr))       
 current_expr = []      
 expression_started = False 
  if expression_started and len(current_expr) > 3: 
    expression.append(''.join(current_expr)) 
 return expression 
#Input 
#input ("enter paragraph with math expression")
paragraph = "In physics, force is calculated using\ 
 F = m*a and energy is given by E = m * c ^ 2" 
#Processing 
tokens = tokenize(paragraph) expressions = extract_math_expressions(tokens) 
print("\nMathematical expressions found:") 
if expressions:   for expr in expressions: 
    print(expr) else: 
  print("No math expressions found.") 
 
